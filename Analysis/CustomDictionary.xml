<Dictionary
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="CustomDictionary.xsd">
  <Words>
    <Recognized>
      <!-- Actual new words. -->
      <Word>monospace</Word>
      <Word>XPS</Word>
      <Word>lorem</Word>
      <Word>ipsum</Word>
      <!-- Single character words: X and Y coordinates, C for char. -->
      <Word>c</Word>
      <Word>x</Word>
      <Word>y</Word>
      <!-- Element names. -->
      <Word>br</Word>
      <Word>div</Word>
      <!-- Method and type names used in exception messages. -->
      <Word>LineThickness</Word>
      <Word>MeasureOverride</Word>
      <Word>StringWriter</Word>
      <!-- My stupid prefixes and suffixes. -->
      <Word>Cs</Word>
      <Word>Exts</Word>
    </Recognized>
    <DiscreteExceptions>
      <!-- Actual exceptions. -->
      <Term>TopLine</Term>
      <Term>LightShade</Term>
      <!-- Method and type names used in exception messages. -->
      <Term>LineThickness</Term>
      <Term>MeasureOverride</Term>
      <Term>StringWriter</Term>
    </DiscreteExceptions>
  </Words>
  <Acronyms>
    <CasingExceptions>
      <Acronym>Cs</Acronym>
    </CasingExceptions>
  </Acronyms>
</Dictionary>